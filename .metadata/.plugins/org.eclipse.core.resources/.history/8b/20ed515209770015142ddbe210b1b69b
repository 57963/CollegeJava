		frame.setDefaultCloseOperation(EXIT_ON_CLOSE);
		frame.setVisible(true);
	}
	
	public void paintComponent(Graphics g){
		g.fillRect(20, 20, 100, 200);
	}
	
	public void draw(int i){
		
	}
	
	public static void main(String[] args){
		new GUI();
	}
}
