package com.as.cs.exercises.areas;

import com.as.cs.exercises.areas.AreasController.Shape;

public class Areas {
	private static float var1;
	private static float var2;
	private static Shape shape;
	Areas (float var1, float var2, Shape shape){
		Areas.var1 = var1;
		Areas.var2 = var2;
		Areas.shape = shape;
	}
	public float getVar1() {
		return var1;
	}
	public void setVar1(float var1) {
		this.var1 = var1;
	}
	public float getVar2() {
		return var2;
	}
	public void setVar2(float var2) {
		this.var2 = var2;
	}
	public Shape getShape() {
		return shape;
	}
	public void setShape(Shape shape) {
		this.shape = shape;
	}
}
